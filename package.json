  "keywords": [
    "json",
    "memory-efficient",
    "lazy-loading",
    "proxy",
    "low-memory",
    "gc-friendly",
    "partial-deserialization",
    "compression",
    "express",
    "middleware",
    "bandwidth",
    "optimization",
    "api",
    "rest",
    "keys",
    "minify",
    "json-compression",
    "payload",
    "reduce",
    "compact",
    "performance",
    "network",
    "nodejs",
    "node",
    "fetch",
    "axios",
    "react-query",
    "swr",
    "angular",
    "mongodb",
    "mongoose",
    "postgresql",
    "pg",
    "mysql",
    "mysql2",
    "sqlite",
    "better-sqlite3",
    "sequelize",
    "orm",
    "sql",
    "database",
    "key-compression",
    "api-optimization",
    "lightweight",
    "transparent"
  ],
  "author": "Tim Carter",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/timclausendev-web/tersejson"
  },
  "bugs": {
    "url": "https://github.com/timclausendev-web/tersejson/issues"
  },
  "homepage": "https://tersejson.com",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./express": {
      "types": "./dist/express.d.ts",
      "import": "./dist/express.mjs",
      "require": "./dist/express.js"
    },
    "./client": {
      "types": "./dist/client.d.ts",
      "import": "./dist/client.mjs",
      "require": "./dist/client.js"
    },
    "./integrations": {
      "types": "./dist/integrations.d.ts",
      "import": "./dist/integrations.mjs",
      "require": "./dist/integrations.js"
    },
    "./analytics": {
      "types": "./dist/analytics.d.ts",
      "import": "./dist/analytics.mjs",
      "require": "./dist/analytics.js"
    },
    "./graphql": {
      "types": "./dist/graphql.d.ts",
      "import": "./dist/graphql.mjs",
      "require": "./dist/graphql.js"
    },
    "./graphql-client": {
      "types": "./dist/graphql-client.d.ts",
      "import": "./dist/graphql-client.mjs",
      "require": "./dist/graphql-client.js"
    },
    "./server-memory": {
      "types": "./dist/server-memory.d.ts",
      "import": "./dist/server-memory.mjs",
      "require": "./dist/server-memory.js"
    },
    "./mongodb": {
      "types": "./dist/mongodb.d.ts",
      "import": "./dist/mongodb.mjs",
      "require": "./dist/mongodb.js"
    },
    "./pg": {
      "types": "./dist/pg.d.ts",
      "import": "./dist/pg.mjs",
      "require": "./dist/pg.js"
    },
    "./mysql": {
      "types": "./dist/mysql.d.ts",
      "import": "./dist/mysql.mjs",
      "require": "./dist/mysql.js"
    },
    "./sqlite": {
      "types": "./dist/sqlite.d.ts",
      "import": "./dist/sqlite.mjs",
      "require": "./dist/sqlite.js"
    },
    "./sequelize": {
      "types": "./dist/sequelize.d.ts",
      "import": "./dist/sequelize.mjs",
      "require": "./dist/sequelize.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest",
    "test:run": "vitest run",
    "lint": "eslint src/",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "npm run build"
  },
  "devDependencies": {
    "@types/better-sqlite3": "^7.6.13",
    "@types/express": "^4.17.21",
    "@types/node": "^20.11.0",
    "@types/pg": "^8.16.0",
    "eslint": "^8.56.0",
    "express": "^4.18.2",
    "mongodb": "^7.0.0",
    "mysql2": "^3.16.0",
    "sequelize": "^6.37.7",
    "sharp": "^0.34.5",
    "tsup": "^8.0.1",
    "typescript": "^5.3.3",
    "vitest": "^1.2.0"
  },
  "peerDependencies": {
    "@apollo/client": ">=3.0.0",
