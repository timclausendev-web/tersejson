{
  "name": "tersejson",
  "version": "0.2.1",
  "description": "Transparent JSON key compression for Express APIs. Reduce bandwidth by up to 80% with zero code changes.",
  "keywords": [
    "json",
    "compression",
    "express",
    "middleware",
    "bandwidth",
    "optimization",
    "api",
    "rest",
    "keys",
    "minify",
    "json-compression",
    "payload",
    "reduce",
    "compact",
    "shrink",
    "performance",
    "network",
    "http",
    "response",
    "nodejs",
    "node",
    "fetch",
    "axios",
    "react-query",
    "swr",
    "angular",
    "restful",
    "key-compression",
    "json-minify",
    "api-optimization",
    "payload-compression",
    "lightweight",
    "proxy",
    "transparent"
  ],
  "author": "Tim Carter",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/timclausendev-web/tersejson"
  },
  "bugs": {
    "url": "https://github.com/timclausendev-web/tersejson/issues"
  },
  "homepage": "https://tersejson.com",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./express": {
      "types": "./dist/express.d.ts",
      "import": "./dist/express.mjs",
      "require": "./dist/express.js"
    },
    "./client": {
      "types": "./dist/client.d.ts",
      "import": "./dist/client.mjs",
      "require": "./dist/client.js"
    },
    "./integrations": {
      "types": "./dist/integrations.d.ts",
      "import": "./dist/integrations.mjs",
      "require": "./dist/integrations.js"
    },
    "./analytics": {
      "types": "./dist/analytics.d.ts",
      "import": "./dist/analytics.mjs",
      "require": "./dist/analytics.js"
    },
    "./graphql": {
      "types": "./dist/graphql.d.ts",
      "import": "./dist/graphql.mjs",
      "require": "./dist/graphql.js"
    },
    "./graphql-client": {
      "types": "./dist/graphql-client.d.ts",
      "import": "./dist/graphql-client.mjs",
      "require": "./dist/graphql-client.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest",
    "test:run": "vitest run",
    "lint": "eslint src/",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "npm run build"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.11.0",
    "eslint": "^8.56.0",
    "express": "^4.18.2",
    "sharp": "^0.34.5",
    "tsup": "^8.0.1",
    "typescript": "^5.3.3",
    "vitest": "^1.2.0"
  },
  "peerDependencies": {
    "express": ">=4.0.0",
    "@apollo/client": ">=3.0.0"
  },
  "peerDependenciesMeta": {
    "express": {
      "optional": true
    },
    "@apollo/client": {
      "optional": true
    }
  },
  "engines": {
    "node": ">=16.0.0"
  }
}
